{
  "version": 2,
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "status": 404,
      "dest": "/404.html"
    },
    {
      "src": "/images/(.*)",
      "headers": { "cache-control": "s-maxage=604800" },
      "dest": "/images/$1"
    },
    {
      "src": "/css/(.*)",
      "headers": { "cache-control": "s-maxage=604800" },
      "dest": "/css/$1"
    }
  ]
}