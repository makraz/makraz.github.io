{
  "version": 2,
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "status": 404,
      "dest": "/404.html"
    }
  ],
  "headers": [
    {
      "source": "/images/(.*)",
      "headers": {
        "key": "Cache-Control",
        "value": "public, max-age=604800, must-revalidate"
      }
    },
    {
      "source": "/css/(.*)",
      "headers": {
        "key": "Cache-Control",
        "value": "public, max-age=604800, must-revalidate"
      }
    }
  ]
}